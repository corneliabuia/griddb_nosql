cod = input('Introduceti codul de 9 cifre:')
sum = 0
prod = 1
prima_cifra = int(cod) / 100000000

while int(cod)/10 != 0:
    cifra = int(cod) % 10
    if int(cifra) % 2 != 0:
        sum = sum + cifra
    else:
        prod = prod*cifra
    cod = int(cod)/10

if sum % int(prima_cifra) == prod % int(prima_cifra):
        print('Corect')
else:
        print('Incorect')
